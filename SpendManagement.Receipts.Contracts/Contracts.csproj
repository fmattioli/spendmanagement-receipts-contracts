<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>SpendManagement.Receipts.Contracts</PackageId>
    <Version>1.25.4</Version>
    <PackageIcon>1.jpg</PackageIcon>
    <Company>SpendManagement</Company>
    <Description>The SpendManagement.Receitps.Contracts was created with the purpose of being a package that contains the commands and the events definitions related to the SpendManagement project on Receipts boundary. This package also contains some web references to the Receipts boundary.</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Flurl" Version="4.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="8.0.6" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="8.0.3" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="7.6.0" />
    <PackageReference Include="Keycloak.AuthServices.Authentication" Version="2.5.1" />
    <PackageReference Include="Keycloak.AuthServices.Authorization" Version="2.5.1" />
  </ItemGroup>
  <ItemGroup>
    <None Update="1.jpg">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>
